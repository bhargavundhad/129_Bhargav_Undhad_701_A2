<!DOCTYPE html>
<html>
<head>
  <title>Registration Result</title>
</head>
<body>
  <h2>Registration Successful</h2>
  <table border="1">
    <tr><th>Username</th><td><%= data.username %></td></tr>
    <tr><th>Email</th><td><%= data.email %></td></tr>
    <tr><th>Gender</th><td><%= data.gender %></td></tr>
    <tr><th>Hobbies</th><td><%= data.hobbies %></td></tr>
    <tr>
      <th>Profile Picture</th>
      <td><img src="/uploads/<%= data.profilePic %>" width="100"></td>
    </tr>
    <tr>
      <th>Other Pictures</th>
      <td>
        <% data.otherPics.forEach(pic => { %>
          <img src="/uploads/<%= pic %>" width="100">
        <% }) %>
      </td>
    </tr>
  </table>
</body>
</html>